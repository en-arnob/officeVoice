<div class="container">
  <nav
    class="rounded-pill px-2 py-2 mt-2 navbar navbar-expand-lg navbar-light"
    style="background-color: #FBF46D;"
  >
    <div class="container-fluid">
      <a class="navbar-brand text-success fw-bold" href="/">
        officeVoice
      </a>
      <button
        class="navbar-toggler"
        type="button"
        data-bs-toggle="collapse"
        data-bs-target="#navbarSupportedContent"
        aria-controls="navbarSupportedContent"
        aria-expanded="false"
        aria-label="Toggle navigation"
      >
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="navbarSupportedContent">
        <ul class="navbar-nav me-auto mb-2 mb-lg-0">
          <li class="nav-item">
            <a class="nav-link active" aria-current="page" href="#">
              Feed
            </a>
          </li>
         <% if (isLoggedIn) { %>
         <li class="nav-item">
            <a class="nav-link" href="/auth/logout">
              Logout
            </a>
          </li>
         <% } else { %>
          <li class="nav-item">
            <a class="nav-link" href="/auth/login">
              Login
            </a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="/auth/signup">
              Create an account
            </a>
          </li>
         <% } %>
        </ul>
        <form class="d-flex">
          <input
            class="form-control me-2 rounded-pill"
            type="search"
            placeholder="Keyword"
            aria-label="Search"
          />
          <button class="btn btn-outline-success rounded-pill" type="submit">
            Search
          </button>
        </form>
      </div>
    </div>
  </nav>
</div>